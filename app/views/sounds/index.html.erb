<h1>Listing sounds</h1>

<table>
  <tr>
    <th>Configuration</th>
    <th>Example</th>
  </tr>

<% for sound in @sounds %>
  <tr>
    <td><%= link_to_name sound.configuration %></td>
    <td><%= link_to_name sound.example %></td>
    <td><%= link_to 'Show', sound %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New sound', new_sound_path %>
